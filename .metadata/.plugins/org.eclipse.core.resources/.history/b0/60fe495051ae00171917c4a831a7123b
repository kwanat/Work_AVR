#include <avr/io.h>
# include <util/delay.h>


int main()
{
	DDRA=0xFF;;
		//uint8_t tab[]={1,3,6,12,24,48,96,192,128,0,128,192,96,48,24,12,6,3,1,0};


	while(1)
	{

			PORTA|=(1<<PA0);
			for(int i=0;i<20;i++)
			{
				PORTA ^= (1<<PA0);
			_delay_ms(50);
			}
			PORTA&=(0<<PA0);
			PORTA|=(1<<PA1);
			for(int i=0;i<20;i++)
			{
				PORTA ^= (1<<PA1);
				_delay_ms(50);
			}
			PORTA&=(0<<PA1);
	}
	return 0;
	}
